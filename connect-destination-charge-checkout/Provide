>         [HttpPost("webhook")]
        public async Task<IActionResult> Webhook()
        {
            var json = await new StreamReader(HttpContext.Request.Body).ReadToEndAsync();
            Event stripeEvent;
            try
            {
                stripeEvent = EventUtility.ConstructEvent(
                    json,
                    Request.Headers["Stripe-Signature"],
                    this.options.Value.WebhookSecret
                );
                Console.WriteLine($"Webhook notification with type: {stripeEvent.Type} found for {stripeEvent.Id}");
            }
            catch (Exception e)
            {
                Console.WriteLine($"Something failed {e}");
                return BadRequest();
            }


            if (stripeEvent.Type == Events.IdentityVerificationSessionVerified) {
              var verificationSession = stripeEvent.Data.Object as VerificationSession;
              // All the verification checks passed

            } else if (stripeEvent.Type == Events.Ident>         public async Task<IActionResult> Webhook()
